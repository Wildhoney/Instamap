{
  "name": "instamap",
  "version": "0.1.0",
  "description": "Towards the end of 2016, Instagram removed the Photo Maps feature. Instagram says it was unused, but we remember! Instamap brings it back for good, open-source and ad-free.",
  "scripts": {
    "test": "ava",
    "build": "NODE_ENV=production webpack",
    "watch": "webpack --watch",
    "cleanup": "rimraf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Instamap.git"
  },
  "keywords": [
    "instagram map",
    "instagram photo map",
    "instagram location",
    "instagram geolocation"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Instamap/issues"
  },
  "homepage": "https://github.com/Wildhoney/Instamap#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "express": "^4.14.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "node-sass": "^4.1.1",
    "opener": "^1.4.2",
    "react-addons-test-utils": "^15.4.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.3"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "ava": {
    "files": [
      "src/*.test.js",
      "src/**/*.test.js",
      "src/**/**/*.test.js",
      "src/**/**/**/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "ignore-styles",
      "babel-polyfill",
      "babel-register",
      "./tests/setup-browser-env.js"
    ]
  }
}
